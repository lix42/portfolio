{
  "name": "@portfolio/document-processor",
  "version": "1.0.0",
  "description": "Event-driven document processing with Durable Objects",
  "type": "module",
  "main": "./src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "node -e \"\"",
    "deploy": "wrangler deploy --minify --env staging",
    "deploy:prod": "wrangler deploy --minify --env production",
    "cf-typegen": "wrangler types",
    "typecheck": "tsc --noEmit",
    "tail": "wrangler tail",
    "tail:staging": "wrangler tail --env staging",
    "tail:prod": "wrangler tail --env production"
  },
  "dependencies": {
    "@portfolio/shared": "workspace:*",
    "openai": "^6.17.0"
  },
  "devDependencies": {
    "@types/node": "^22.19.7",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18",
    "wrangler": "^4.61.1"
  }
}
