{
  "name": "@portfolio/container-proto",
  "version": "0.1.0",
  "type": "module",
  "//": "build/test are no-ops: wrangler compiles TS directly, no separate build needed",
  "scripts": {
    "dev": "wrangler dev --port 8787",
    "build": "node -e \"\"",
    "test": "node -e \"\"",
    "deploy": "wrangler deploy",
    "typecheck": "tsc --noEmit",
    "cf-typegen": "wrangler types"
  },
  "dependencies": {
    "@cfworker/json-schema": "^4.1.1",
    "@cloudflare/containers": "^0.1.0",
    "json-schema-to-ts": "^3.1.1"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "wrangler": "^4.61.1"
  }
}
