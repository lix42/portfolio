{
  "name": "@portfolio/ui",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "waku dev",
    "build": "$npm_execpath run eslint:check && tsc --noEmit && waku build",
    "preview": "waku start",
    "lint": "$npm_execpath run eslint:check",
    "lint:check": "eslint .",
    "lint:fix": "$npm_execpath run eslint:fix",
    "cf-typegen": "wrangler types -c ./wrangler.jsonc -c ../service/wrangler.jsonc",
    "type:check:ci": "tsc --noEmit",
    "eslint": "$npm_execpath run eslint:check",
    "eslint:check": "eslint .",
    "eslint:fix": "eslint --fix ."
  },
  "dependencies": {
    "@portfolio/shared": "workspace:*",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-server-dom-webpack": "19.2.0",
    "waku": "0.27.1"
  },
  "devDependencies": {
    "@portfolio/eslint-config": "workspace:*",
    "@tailwindcss/postcss": "4.1.10",
    "@types/node": "^24.7.2",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "eslint": "^9.37.0",
    "postcss": "8.5.6",
    "prettier": "^3.4.2",
    "tailwindcss": "4.1.10",
    "typescript": "5.8.3",
    "wrangler": "^4.45.0"
  }
