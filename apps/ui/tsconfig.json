{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "esnext",
    "noEmit": true,
    "isolatedModules": true,
    "moduleDetection": "force",
    "downlevelIteration": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "skipLibCheck": true
  },
  "include": ["src/**/*", "service-bindings.d.ts", "worker-configuration.d.ts"]
}
