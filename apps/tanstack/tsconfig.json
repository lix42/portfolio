{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "lib": ["DOM", "DOM.Iterable", "ES2022"],
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["./src/*"],
      "@service/*": ["../service/*"]
    },
    "noEmit": true
  },
  "include": ["src", "vite.config.ts", "worker-configuration.d.ts"],
  "references": []
}
